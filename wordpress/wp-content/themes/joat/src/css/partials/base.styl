*
  box-sizing border-box
  margin 0
  padding 0
html
	font-size font-size
	scroll-behavior: smooth;
body
  background-color #fff
  color the-copy
  font-family font-default
  font-weight font-weight
  line-height 1.6
button
	cursor: pointer
a
  color the-copy
  cursor pointer
  font-weight bold
  transition .3s
iframe
  display block
iframe,img,video
  max-width 100%
img,video
  height auto
main
  display block
p,ul,ol
  margin-bottom 1em
ul,ol
  margin-left 0
